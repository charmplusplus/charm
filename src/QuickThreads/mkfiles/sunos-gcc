
CC=gcc -I. -O2

all: qt stp run

qt: md/sparc.h qt.c md/sparc.s
	rm -f qtmd.h
	ln -s md/sparc.h qtmd.h
	$(CC) -pedantic -Wall -c -o qt.o qt.c
	as -o qtmds.o md/sparc.s
	ar q libqt.a qt.o qtmds.o
	ranlib libqt.a

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o
	ranlib libqt.a

testpgm:
	$(CC) -c meas.c
	as -o qtmdb.o md/sparc_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a
