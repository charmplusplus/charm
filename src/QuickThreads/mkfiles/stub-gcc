
CC=gcc -I. -O2

all: qt stp testpgm

qt: md/stub.h qt.c md/stub.c
	rm -f qtmd.h
	ln -s md/stub.h qtmd.h
	$(CC) -pedantic -Wall -c -o qt.o qt.c
	$(CC) -pedantic -Wall -c -o qtmdc.o md/stub.c
	ar q libqt.a qt.o qtmdc.o

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	$(CC) -c -o qtmdb.o md/stub_b.c
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a
	

