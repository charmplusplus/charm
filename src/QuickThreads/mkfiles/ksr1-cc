
CC=cc -ansi -I. -O

all: qt stp testpgm

qt: md/ksr1.h qt.c md/ksr1.s
	rm -f qtmd.h
	ln -s md/ksr1.h qtmd.h
	$(CC) -c -o qt.o qt.c
	as -o qtmds.o md/ksr1.s
	ar cr libqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar cr libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	as -o qtmdb.o md/ksr1_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libqt.a

	
